<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Evaluation">


    <EditText
        android:id="@+id/editnickname"
        android:layout_width="371dp"
        android:layout_height="57dp"
        android:layout_marginTop="25dp"
        android:ems="10"
        android:hint="Nickname"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/selectedImage"
        android:layout_width="114dp"
        android:layout_height="103dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editnickname"
        app:srcCompat="@drawable/select_avatar" />

    <TextView
        android:id="@+id/textViewstudyprogram"
        android:layout_width="271dp"
        android:layout_height="32dp"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:text="Select your study program"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/selectedImage" />

    <RadioGroup
        android:id="@+id/group"
        android:layout_width="196dp"
        android:layout_height="109dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewstudyprogram">

        <RadioButton
            android:id="@+id/radioButtonGraduate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Graduate" />

        <RadioButton
            android:id="@+id/radioUndergraduate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Undergraduate" />

    </RadioGroup>

    <TextView
        android:id="@+id/textViewWellStructured"
        android:layout_width="305dp"
        android:layout_height="42dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="44dp"
        android:layout_marginRight="44dp"
        android:text="Class is Well Structured"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/group" />

    <TextView
        android:id="@+id/textViewdeadlines"
        android:layout_width="314dp"
        android:layout_height="42dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:text="assignment deadlines is reasonable"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBarStructure" />

    <TextView
        android:id="@+id/textViewcourcemeterial"
        android:layout_width="314dp"
        android:layout_height="42dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:text="Course meterial is aligned with assignments"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBardeadline" />

    <SeekBar
        android:id="@+id/seekBarStructure"
        android:layout_width="255dp"
        android:layout_height="41dp"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:max="5"
        android:min="1"
        android:progress="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewWellStructured" />

    <SeekBar
        android:id="@+id/seekBardeadline"
        android:layout_width="255dp"
        android:layout_height="41dp"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:max="5"
        android:min="1"
        android:progress="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewdeadlines" />

    <SeekBar
        android:id="@+id/seekBarcourse"
        android:layout_width="246dp"
        android:layout_height="35dp"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:max="5"
        android:progress="1"
        android:min="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewcourcemeterial" />

    <Button
        android:id="@+id/buttonSubmit"
        android:layout_width="173dp"
        android:layout_height="56dp"
        android:layout_marginStart="115dp"
        android:layout_marginLeft="115dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="115dp"
        android:layout_marginRight="115dp"
        android:text="Submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBarcourse" />

    <TextView
        android:id="@+id/textViewStructure"
        android:layout_width="85dp"
        android:layout_height="41dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintStart_toEndOf="@+id/seekBarStructure"
        app:layout_constraintTop_toBottomOf="@+id/textViewWellStructured" />

    <TextView
        android:id="@+id/textViewdeadline"
        android:layout_width="82dp"
        android:layout_height="38dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintStart_toEndOf="@+id/seekBardeadline"
        app:layout_constraintTop_toBottomOf="@+id/textViewdeadlines" />

    <TextView
        android:id="@+id/textViewassignment"
        android:layout_width="82dp"
        android:layout_height="38dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintStart_toEndOf="@+id/seekBarcourse"
        app:layout_constraintTop_toBottomOf="@+id/textViewcourcemeterial" />


</androidx.constraintlayout.widget.ConstraintLayout>